{
  "name": "solana-nextjs-starter",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "yarn run dev:default",
    "dev:open": "open http://localhost:3000 && yarn run dev",
    "dev:default": "cross-env FORCE_COLOR=1 next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prettier": "prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "prettier:fix": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "analyze": "cross-env ANALYZE=true yarn build",
    "storybook": "cross-env FORCE_COLOR=1 storybook dev -p 6006",
    "test-storybook": "cross-env FORCE_COLOR=1 test-storybook",
    "build-storybook": "cross-env FORCE_COLOR=1 storybook build",
    "test": "cross-env FORCE_COLOR=1 jest --passWithNoTests",
    "e2e:headless": "playwright test",
    "e2e:ui": "playwright test --ui",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "postinstall": "npx patch-package -y",
    "preinstall": "npx only-allow yarn",
    "clean": "yarn run clean:build",
    "clean:build": "yarn run del-cli ./build/",
    "clean:all": "yarn run clean:build && yarn run clean:next && yarn run clean:node",
    "clean:ALL": "yarn run clean:scripts:ALL && yarn run clean:all && yarn run clean:npm",
    "clean:next": "yarn run del-cli .next",
    "clean:next:cache": "yarn run del-cli .next/.cache",
    "clean:node": "yarn run del-cli ./node_modules/",
    "clean:npm": "npm cache clean --force && npm cache verify",
    "clean:scripts:ALL": "yarn run clean:scripts-dumps && yarn run clean:scripts-validator-data",
    "clean:scripts-dumps": "yarn run del-cli ./scripts/dumps",
    "clean:scripts-validator-data": "yarn run del-cli ./scripts/test-ledger"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/next-js": "^2.2.0",
    "@chakra-ui/react": "^2.8.2",
    "@coral-xyz/anchor": "0.29.0",
    "@coral-xyz/borsh": "^0.30.1",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@hookform/resolvers": "^3.1.1",
    "@metaplex-foundation/js": "^0.20.1",
    "@metaplex-foundation/mpl-candy-guard": "^0.5.0",
    "@metaplex-foundation/mpl-core": "^1.0.2",
    "@metaplex-foundation/mpl-core-candy-machine": "^0.2.1",
    "@metaplex-foundation/mpl-token-metadata": "^3.2.1",
    "@metaplex-foundation/umi": "^0.9.2",
    "@metaplex-foundation/umi-bundle-defaults": "^0.9.2",
    "@metaplex-foundation/umi-signer-wallet-adapters": "^0.9.2",
    "@metaplex-foundation/umi-uploader-irys": "^0.9.2",
    "@metaplex-foundation/umi-uploader-nft-storage": "^0.9.2",
    "@next/bundle-analyzer": "^14.2.4",
    "@project-serum/anchor": "^0.26.0",
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^10.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.0.0",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^11.0.1",
    "@solana/spl-token": "^0.4.8",
    "@solana/wallet-adapter-base": "^0.9.22",
    "@solana/wallet-adapter-react": "^0.15.32",
    "@solana/wallet-adapter-react-ui": "^0.9.31",
    "@solana/wallet-adapter-wallets": "^0.19.16",
    "@solana/web3.js": "^1.95.1",
    "classnames": "^2.3.2",
    "framer-motion": "^11.2.13",
    "lodash": "^4.17.21",
    "lucide-react": "^0.244.0",
    "next": "^14.2.4",
    "next-compose-plugins": "^2.2.1",
    "next-seo": "^6.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.44.3",
    "react-qrcode-logo": "^3.0.0",
    "tailwind-merge": "^1.10.0",
